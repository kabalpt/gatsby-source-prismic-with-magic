{
  "name": "gatsby-source-prismic-with-magic",
  "version": "1.0.7",
  "description": "Gatsby plugin that sources prismic data and creates magic stuff",
  "main": "index.js",
  "repository": "git@github.com:kabalpt/gatsby-source-prismic-with-magic.git",
  "author": "<kabal.dev.pt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.2.1",
    "eslint-config-kabalpt-react": "^1.0.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "lodash": "^4.17.15",
    "prismic-javascript": "^2.1.1"
  }
}
